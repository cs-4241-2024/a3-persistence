<div class="block">
  <h1>Active Laptop Loans</h1>
  <form>
    <input type="text" id="id" value="Laptop ID" aria-label="Laptop ID number entry">
    <input type="text" id="firstname" value="First Name" aria-label="Laptop recipient first name entry">
    <input type="text" id="lastname" value="Last Name" aria-label="Laptop recipient last name entry">
    <button id="submitbtn" class="removebtn">Submit</button>
  </form>

  <table id="laptops">
    <tr>
      <th>Laptop ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Duplicate Client?</th>
      <th class="removecol"></th>
    </tr>
  </table>

  <div>
    <button id="rfrsh" class="refreshbtn">Pull from Server</button>
    <button id="logout" class="refreshbtn">Log Out</button>
  </div>
</div>
